{
    "name": "edarkzero/cmsbuilder",
    "description": "Build your website pages with this complete CMS builder, where you can add multiple dynamic fields or add you own fields.",
    "license": "MIT",
    "authors": [
        {
            "name": "Edgar Orlando Cardona Hernandez",
            "email": "edgarcardona87@gmail.com",
            "homepage": "http://www.cardonait.com"
        }
    ],
    "homepage": "https://github.com/edarkzero/cmsbuilder/",
    "keywords": ["Laravel", "CMSBuilder"],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Edarkzero\\CMSBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Edarkzero\\CMSBuilder\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Edarkzero\\CMSBuilder\\CMSBuilderServiceProvider"
            ],
            "aliases": {
                "CMSBuilder": "Edarkzero\\CMSBuilder\\Facades\\CMSBuilder"
            }
        }
    }
}